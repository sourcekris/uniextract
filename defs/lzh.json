{
    "name": "lzh",
    "extensions": ["lzh", "lha"],
    "install": {
        "method": "apt",
        "packages": [
            "atool",
            "lhasa",
            "jlha-utils"
        ]
    },
    "pack": {
        "exe": "jlha",
        "cmdline": "$tool -aq9 $file.lzh $file",
        "type": "archiver"
    },
