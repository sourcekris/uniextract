{
    "name": "uuencode",
    "extensions": ["uue", "uu"],
    "install": {
        "method": "apt",
        "packages": ["sharutils"]
    },
    "pack": {
        "exe": "uuencode",
        "cmdline": "$tool $file $file > $file.uue",
        "type": "archiver"
    },
    "unpack": {
        "exe": "uudecode",
        "cmdline": "cd $destdir && $tool $archive",
        "extension": "uue",
        "force_extension": true
    },
    "test": {
        "blob": "H4sIAIpgzGEAA0tKTc/MUzAzMVEw4FI2NTVV5UrgSs1L4QIA/50+/xgAAAA=",
        "file": "0",
        "content": "UUE",
        "delete": true
    },
    "identification": { "file": "uuencoded or xxencoded, ASCII text", "trid": "(.UUE/UU/XXE) UUencoded/XXencoded text" }
}