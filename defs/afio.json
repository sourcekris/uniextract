{
    "name":"AFIO",
    "comment":"afio is like cpio but supports compression - http://fileformats.archiveteam.org/wiki/Afio",
    "extensions":["afio", "af"],
    "install": {
        "method":"apt",
        "packages":["afio"]
    },
    "pack":{
        "exe":"afio",
        "cmdline":"echo $file | $tool -o $file.cpio"
    },
    "unpack": {
        "exe":"afio",
        "cmdline":"cd $destdir && $tool -i $archive",
        "extension":"afio",
        "force_extension": true
    },
