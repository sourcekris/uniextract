{
    "name":"Zoo",
    "comment":"Zoo archiver, popularized on Amiga and OpenVMS - http://fileformats.archiveteam.org/wiki/Zoo",
    "extensions":["zoo"],
    "install": {
        "method":"source",
        "repo":"https://github.com/jsummers/deark",
        "build":"make && cp deark $tools",
        "exist_check":["deark -version","Deark version:"]
    },
    "unpack": {
        "exe":"$tools/deark",
        "cmdline":"$tool -od $destdir -o 0 $archive",
        "extension":"zoo",
        "force_extension": true
