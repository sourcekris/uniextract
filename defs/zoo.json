{
    "name":"Zoo",
    "comment":"Zoo archiver, popularized on Amiga and OpenVMS - http://fileformats.archiveteam.org/wiki/Zoo",
    "extensions":["zoo"],
    "install": {
        "method":"source",
        "repo":"https://github.com/troglobit/zoo",
        "build":"./autogen.sh && ./configure --enable-ooz && make && cp src/zoo src/ooz src/unzoo $tools",
        "exist_check":["zoo -h","zoo COMMAND"]
    },
    "pack":{
      "type":"archiver",
      "exe":"$tools/zoo",
      "cmdline":"$tool a $file.$extension $file"
    },
    "unpack": {
        "exe":"$tools/zoo",
        "cmdline":"cd $destdir && $tool e $archive",
        "extension":"zoo",
        "force_extension": true
    },
    "test": {
