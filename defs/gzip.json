{
    "name": "Gzip",
    "extensions": ["gz", "tgz"],
    "install": {
        "method": "apt",
        "packages": ["atool"]
    },
    "pack": {
        "exe": "gzip",
        "cmdline": "$tool $file",
        "type": "archiver"
    },
    "unpack": {
        "exe": "atool",
        "cmdline": "$tool --extract-to $destdir $archive",
        "extension": "gz",
        "force_extension": true
    },
    "test": {
        "blob": "H4sIAIcOy2EAA5Pv5mAAA+bifiaG68XxpUxAPgBupYmVFgAAAA==",
        "file": "?",
        "content": "GZ",
        "delete": false
    },
    "identification": {
        "file": "gzip compressed data, was ",
        "trid": "(.GZ/GZIP) GZipped data",
        "idarc": "GZIP/Comp4.3"
    }
}