{
    "name":"ADF",
    "comment":"Amiga floppy disk format",
    "extensions":["adf"],
    "install": {
        "method":"apt",
        "packages":["unar"]
    },
    "pack":{
        "exe":"xdftool",
        "cmdline":"$tool $file.adf create + format $file + write $file"
    },
    "unpack": {
        "exe":"unar",
        "cmdline":"$tool -o $destdir $archive",
        "extension":"adf",
        "force_extension": true
    },
    "test": {
        "blob":"",
        "file":"0",
        "content":"ADF",
        "delete":true
    }
}