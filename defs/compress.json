{
    "name": "compress",
    "extensions": ["Z"],
    "install": {
        "method": "apt",
        "packages": ["atool", "ncompress"]
    },
    "pack": {
        "exe": "compress",
        "cmdline": "$tool -f $file",
        "type": "archiver"
    },
    "unpack": {
        "exe": "atool",
        "cmdline": "$tool --extract-to=$destdir $archive",
        "extension": "Z",
        "force_extension": true
    },
    "test": {
        "blob": "H4sIAJ78y2EAA5OfOyGKAQAZw//WBQAAAA==",
        "file": "?",
        "content": "Z",
        "delete": false
    },
    "identification": { "file": "compress'd data 16 bits", "trid": " (.Z) UNIX Compressed data" }
}