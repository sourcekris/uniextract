{
    "name": "HDF",
    "comment": "Amiga hard disk format",
    "extensions": ["hdf"],
    "install": {
        "method": "pip",
        "packages": ["amitools"]
    },
    "pack": {
        "exe": "xdftool",
        "cmdline": "$tool $file.hdf create size=1M + format $file + write $file",
        "type": "archiver"
    },
    "unpack": {
        "exe": "xdftool",
        "cmdline": "$tool $archive unpack $destdir",
        "extension": "hdf",
        "force_extension": true
    },
    "test": {
        "blob": "",
        "file": "0/0",
        "content": "HDF",
        "delete": true
    }
}