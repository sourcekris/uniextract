{
    "name": "MSI",
    "comment": "Microsoft Installer",
    "extensions": ["msi"],
    "install": {
        "method": "apt",
        "packages": ["unar"]
    },
    "pack": {
        "exe": "msibuild",
        "cmdline": "$tool $file.msi -a $file $file",
        "type": "archiver"
    },
    "unpack": {
        "exe": "unar",
        "cmdline": "$tool -o $destdir $archive",
        "extension": "msi",
        "force_extension": true
    },
    "test": {
        "blob": "",
        "file": "?/0",
        "content": "MSI",
        "delete": true
    },
    "identification": {
        "file": "Composite Document File V2 Document, Little Endian, Os",
        "trid": " (.MST) Windows SDK Setup Transform script (61000/1/5)",
        "idarc": "None"
    }
}