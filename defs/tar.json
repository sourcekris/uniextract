{
    "name": "Tar",
    "extensions": ["tar", "ctar"],
    "install": {
        "method": "apt",
        "packages": ["tar"]
    },
    "pack": {
        "exe": "tar",
        "cmdline": "$tool cf $file.tar $file",
        "type": "archiver"
    },
    "unpack": {
        "exe": "tar",
        "cmdline": "$tool xf $archive -C $destdir",
        "extension": "tar",
        "force_extension": true
    },
