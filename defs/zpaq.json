{
    "name": "ZPAQ",
    "comment": "Zpaq journaling archiver - http://fileformats.archiveteam.org/wiki/ZPAQ",
    "extensions": ["zpaq"],
    "install": {
        "method": "apt",
        "packages": ["zpaq"]
    },
    "pack": {
        "exe": "zpaq",
        "cmdline": "$tool a $file.zpaq $file",
        "type": "archiver"
    },
    "unpack": {
        "exe": "zpaq",
        "cmdline": "$tool x $archive -to $destdir",
        "extension": "zpaq",
        "force_extension": true
    },
    "test": {
        "blob": "",
        "file": "0",
        "content": "ZPAQ",
        "delete": true
    },
    "identification": {
        "file": "ZPAQ file",
        "trid": "(.ZPAQ) zpaq compressed archive",
        "idarc": "None"
    }
}