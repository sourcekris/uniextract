{
    "name": "AR",
    "extensions": ["a", "ar"],
    "install": {
        "method": "apt",
        "packages": ["binutils", "atool"]
    },
    "pack": {
        "exe": "atool",
        "cmdline": "$tool -a -q $file.a $file",
        "type": "archiver"
    },
    "unpack": {
        "exe": "ar",
        "cmdline": "$tool x --output=$destdir $archive",
        "extension": "a",
        "force_extension": true
    },
