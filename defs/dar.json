{
    "name":"Dar",
    "comment":"Disk Archiver - designed as a replacement for TAR - http://dar.linux.free.fr/",
    "extensions":["dar"],
    "install": {
        "method":"apt",
        "packages":["dar"]
    },
    "pack":{
        "exe":"dar",
        "cmdline":"$tool -c $file -X $file.1.dar"
    },
    "unpack": {
        "exe":"dar",
        "cmdline":"mv $archive $destdir/0.1.dar && cd $destdir && $tool -x 0 -q",
        "extension":"dar",
        "force_extension": true
    },
    "test": {
        "blob":"",
        "file":"0",
        "content":"DAR",
        "delete":false
    }
}